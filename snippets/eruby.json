{
  "translate strings": {
    "prefix": "trans",
    "body": "<%= t('$1') %>",
    "description": "none"
  },
  "debug": {
    "prefix": "d",
    "body": "<% binding.pry %>",
    "description": "none"
  },
  "template": {
    "prefix": "o",
    "body": "<% $1 %>",
    "description": "none"
  },
  "template=": {
    "prefix": "o=",
    "body": "<%= $1 %>",
    "description": "make it easy to output something in erb"
  
  }
}
