{
  "print frozen_string_literal comment": {
    "prefix": "frozen",
    "body": "# frozen_string_literal: true",
    "description": "add frozen literal comment to file"
  },
  "rspec-begin": {
    "prefix": "rbegin",
    "body": [
      "# frozen_string_literal: true",
      "",
      "require 'rails_helper'",
      "",
      "RSpec.describe $1 do",
      "$2",
      "end"
    ]
  },
  "rspec-it": {
    "prefix": "rit",
    "body": ["it \"$1\" do", "\t$2", "end"]
  },
  "rspec-describe": {
    "prefix": "rd",
    "body": ["describe \"$1\" do", "\t$2", "end"]
  },
  "rspec-let": {
    "prefix": "rlet",
    "body": ["let(:$1) { $2 }"]
  },
  "rspec-let-do": {
    "prefix": "rdo",
    "body": ["let(:$1) do", "\t$2", "end"]
  },
  "rspec-before": {
    "prefix": "rbefore",
    "body": ["before do", "\t$1", "end"]
  },
  "abstract-method": {
    "prefix": "abs-method",
    "body": [
      "def $1",
      "\traise NotImplementedError, \"Subclass must implement $1\"",
      "end"
    ]
  },
  "some-uuid": {
    "prefix": "-uuid",
    "body": "\"53ab22f4-031a-44da-8568-4854402fe556\""
  },
  "def": {
    "prefix": "def",
    "body": [
      "def $1",
      "end"
    ]
  }
}
